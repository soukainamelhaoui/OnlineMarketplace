<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ma.fstt.marketplace.view.UtilisateurAcPages"
    >

    <FrameLayout
        android:id="@+id/fragments_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toTopOf="@+id/container_items_bottom"
    />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/container_items_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/menu"
        app:itemTextColor="@drawable/bottom_colors"
        app:itemIconTint="@drawable/bottom_colors"
        android:background="@color/bottomColorAc"
    />

</androidx.constraintlayout.widget.ConstraintLayout>